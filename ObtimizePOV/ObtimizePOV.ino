void setup() {
  pixels.begin();        // 네오픽셀 초기화
  pixels.setBrightness(BRIGHTNESS); //밝기설정 
  pixels.clear();        // 모든 LED를 초기화하여 끔
  pixels.show();         // LED 초기화 후 반영
}

void loop() {
  for (int col = 0; col < NUMCOLUMNS; col++) {
    updateColumnColors(col);
    pixels.show();       // 모든 픽셀 업데이트
    delay(DELAY_MS);     // 열 전환 지연
  }
  delay(3);            // 전체 반복 간 짧은 대기
}

// 열 색상을 설정하는 함수
void updateColumnColors(int col) {
  for (int i = 0; i < NUMPIXELS; i++) {
    uint8_t r = pgm_read_byte(&colors[i][col][0]);
    uint8_t g = pgm_read_byte(&colors[i][col][1]);
    uint8_t b = pgm_read_byte(&colors[i][col][2]);
    pixels.setPixelColor(i, pixels.Color(r, g, b));
  }
}